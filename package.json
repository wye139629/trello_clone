    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-jest": "^3.0.2",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "file-loader": "^6.2.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.2",
