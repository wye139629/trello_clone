{
  "name": "trello-clone",
  "version": "1.0.0",
  "description": "A task management app like trello",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "jest -o",
    "test:all": "jest",
    "test:watch": "jest --watch",
    "dev": "cross-env NODE_ENV=development webpack server",
    "lint": "eslint --ignore-path .gitignore .",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|json|jsx)\"",
    "check-format": "yarn run prettier --list-different",
    "format": "yarn run prettier --write",
    "build": "rimraf ./dist && cross-env NODE_ENV=production webpack",
    "validate": "yarn run check-format && yarn run lint && yarn run build",
    "prepare": "husky install"
  },
  "author": "William",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.17.2",
    "@emotion/babel-preset-css-prop": "^11.2.0",
